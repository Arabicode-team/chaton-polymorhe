<p style="color: green"><%= notice %></p>
<p style="color: red"><%= alert %></p>

<div id="photos" class="row d-flex justify-content-around">
<% @photos.each do |photo| %>
  <div class="card border-warning col-md-3 col-lg-3 mx-1 my-5 pb-3">
    <%= render photo %>
    <%= link_to "Show this photo", photo, class: "btn btn-outline-primary btn-sm" %>
    </div>
<% end %>
</div>
